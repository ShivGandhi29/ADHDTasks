{
  "expo": {
    "name": "Focusd",
    "slug": "focusd",
    "description": "Your ADHD to-do list. One task at a time.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon/logo.icon",
    "scheme": "adhdtasks",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adhdtasks.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/icon/logo-android.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.adhdtasks.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon/logo.icon",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d772d567-53f7-452a-8450-17149ae3e20a"
      }
    },
    "owner": "adhdtasks-organisation"
  }
}
